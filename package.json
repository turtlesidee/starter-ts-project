{
  "name": "starter-ts-project",
  "version": "1.0.0",
  "description": "minimal configuration for ts-project back-end",
  "main": "index.js",
  "repository": "git@turtlehub:turtlesidee/starter-ts-project.git",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf dist && tsc",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "husky:prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "validate-branch-name": "^1.2.1"
  },
  "dependencies": {
    "@turtleside/standard": "^0.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^17.0.2",
    "ably": "^1.2.15",
    "aws-sdk": "^2.1047.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "fp-ts": "^2.11.5",
    "helmet": "^4.6.0",
    "io-ts": "^2.2.16",
    "mongodb": "^4.2.2"
  },
  "validate-branch-name": {
    "pattern": "^(master|staging|sandbox){1}$|^(feature|bugfix)/(SHA-\\d+)$",
    "errorMsg": "You do not follow the branch name norm"
  }
}
